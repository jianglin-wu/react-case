{
  "name": "some-case",
  "version": "1.0.0",
  "description": "React 项目模板（基于 create-react-app）",
  "scripts": {
    "analyze": "source-map-explorer 'dist/static/js/*.js'",
    "build": "cross-env REACT_APP_RUNTIME_TARGET=spa node ./scripts/build.js",
    "build:ssr": "npm-run-all build:client-ssr build:server",
    "build:client-ssr": "cross-env REACT_APP_RUNTIME_TARGET=ssr node ./scripts/build.js",
    "build:server": "cross-env NODE_ENV=production babel src --no-babelrc --config-file ./config/ssr-babelrc.js --out-dir lib --ignore \"src/**/*.test.js\"",
    "start": "cross-env REACT_APP_RUNTIME_TARGET=spa node ./scripts/start.js",
    "start:ssr": "npm-run-all --parallel start:server build:client-ssr",
    "start:client-ssr": "cross-env REACT_APP_RUNTIME_TARGET=ssr node ./scripts/start.js",
    "start:server": "nodemon ./config/ssr-develop.js",
    "lint": "eslint --ext .js src && npm run lint:prettier",
    "lint:prettier": "check-prettier lint",
    "prettier": "prettier --write \"**/*\"",
    "test": "npm run lint"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@ant-design/pro-layout": "^4.5.16",
    "@babel/runtime": "^7.6.2",
    "classnames": "^2.2.6",
    "core-js": "^3.2.1",
    "dva": "^2.6.0-beta.14",
    "dva-core": "^2.0.0",
    "dva-loading": "^3.0.14",
    "express-to-koa": "^2.0.0",
    "glob": "^7.1.4",
    "history": "^4.10.1",
    "koa": "^2.8.1",
    "koa-route": "^3.2.0",
    "koa-static": "^5.0.0",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.18.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet-async": "^1.0.3",
    "react-hot-loader": "^4.12.14",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.1.0",
    "redux": "^4.0.4",
    "redux-saga": "^1.1.1",
    "register-service-worker": "^1.6.2",
    "serialize-javascript": "^2.1.0",
    "swagger-stats": "^0.95.11",
    "umi-request": "^1.2.6",
    "workbox-webpack-plugin": "^4.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.6.2",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@hot-loader/react-dom": "^16.9.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-css-modules-transform": "^1.6.2",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-named-asset-import": "^0.3.4",
    "babel-preset-react-app": "^9.0.2",
    "case-sensitive-paths-webpack-plugin": "^2.2.0",
    "check-prettier": "^1.0.3",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "eslint": "^6.4.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "file-loader": "^3.0.1",
    "fs-extra": "^8.1.0",
    "html-webpack-plugin": "^4.0.0-beta.8",
    "husky": "^3.0.9",
    "is-wsl": "^2.1.1",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "lint-staged": "^9.4.2",
    "mini-css-extract-plugin": "^0.8.0",
    "nodemon": "^1.19.2",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "pnp-webpack-plugin": "^1.5.0",
    "postcss-loader": "^3.0.0",
    "postcss-normalize": "^8.0.1",
    "postcss-safe-parser": "^4.0.1",
    "react-dev-utils": "^9.1.0",
    "resolve": "^1.12.0",
    "sass-loader": "^8.0.0",
    "source-map-explorer": "^2.1.0",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^2.1.3",
    "url-loader": "^2.2.0",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.5.1",
    "webpack-manifest-plugin": "^2.2.0",
    "webpackbar": "^4.0.0"
  },
  "checkFiles": [
    "src/**/*.js*",
    "src/**/*.ts*",
    "src/**/*.less"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx}": "eslint --ext .js,.jsx,.ts,.tsx"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
