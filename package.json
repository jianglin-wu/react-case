{
  "name": "some-case",
  "version": "1.0.0",
  "description": "React technical practice.",
  "main": "index.js",
  "scripts": {
    "analyze": "npm run build && source-map-explorer 'dist/*.js'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "npm-run-all --parallel server client",
    "build": "cross-env NODE_ENV=production RUNTIME_TARGET=spa webpack --config webpack.config.js",
    "start": "cross-env NODE_ENV=development RUNTIME_TARGET=spa webpack-dev-server --open --progress",
    "client": "cross-env NODE_ENV=development RUNTIME_TARGET=ssr webpack --watch --config webpack.config.js",
    "server": "nodemon index.js",
    "lint": "eslint --ext .js src && npm run lint:prettier",
    "lint:prettier": "check-prettier lint",
    "prettier": "prettier --write \"**/*\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.6.2",
    "classnames": "^2.2.6",
    "core-js": "^3.2.1",
    "koa": "^2.8.1",
    "koa-route": "^3.2.0",
    "koa-static": "^5.0.0",
    "prettier": "^1.18.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.12.14",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.1.0",
    "redux": "^4.0.4",
    "redux-saga": "^1.1.1",
    "serialize-javascript": "^2.1.0",
    "workbox-webpack-plugin": "^4.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.6.2",
    "@hot-loader/react-dom": "^16.9.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-css-modules-transform": "^1.6.2",
    "babel-plugin-module-resolver": "^3.2.0",
    "check-prettier": "^1.0.3",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "eslint": "^6.4.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
