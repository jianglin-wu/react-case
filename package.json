{
  "name": "some-case",
  "version": "1.0.0",
  "description": "React technical practice.",
  "main": "index.js",
  "scripts": {
    "analyze": "npm run build:client && source-map-explorer 'dist/*.js'",
    "start": "cross-env NODE_ENV=development RUNTIME_TARGET=spa npm-run-all --parallel start:server \"build:client -- -w\"",
    "build": "cross-env NODE_ENV=production RUNTIME_TARGET=spa npm-run-all build:*",
    "build:client": "webpack --config webpack.config.js",
    "build:server": "babel src --no-babelrc --config-file ./babelrc-node.js --out-dir lib --ignore \"src/**/*.test.js\"",
    "start:client": "cross-env NODE_ENV=development RUNTIME_TARGET=spa webpack-dev-server --open --progress",
    "start:server": "nodemon index.js",
    "lint": "eslint --ext .js src && npm run lint:prettier",
    "lint:prettier": "check-prettier lint",
    "prettier": "prettier --write \"**/*\"",
    "test": "npm run lint"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.6.2",
    "classnames": "^2.2.6",
    "core-js": "^3.2.1",
    "dva": "^2.6.0-beta.14",
    "dva-loading": "^3.0.14",
    "express-to-koa": "^2.0.0",
    "glob": "^7.1.4",
    "history": "^4.10.1",
    "koa": "^2.8.1",
    "koa-route": "^3.2.0",
    "koa-static": "^5.0.0",
    "prettier": "^1.18.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet-async": "^1.0.3",
    "react-hot-loader": "^4.12.14",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.1.0",
    "redux": "^4.0.4",
    "redux-saga": "^1.1.1",
    "register-service-worker": "^1.6.2",
    "serialize-javascript": "^2.1.0",
    "swagger-stats": "^0.95.11",
    "umi-request": "^1.2.6",
    "workbox-webpack-plugin": "^4.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.6.2",
    "@hot-loader/react-dom": "^16.9.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-css-modules-transform": "^1.6.2",
    "babel-plugin-module-resolver": "^3.2.0",
    "check-prettier": "^1.0.3",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "eslint": "^6.4.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "nodemon": "^1.19.2",
