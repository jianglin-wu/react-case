    "eslint-plugin-react-hooks": "^1.7.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.19.2",
    "npm-run-all": "^4.1.5",
    "source-map-explorer": "^2.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.5.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jianglin-wu/some-case.git"
